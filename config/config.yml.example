satellite:
  url: https://sat62.example.org
  username: svc-api-user
  password: 1t$a$3cr3t
  default_org: MyOrg
  disconnected: False

logging:
  dir: /var/log/satellite
  debug: False

export:
  dir: /var/sat-export

import:
  dir: /var/sat-content
  syncbatch: 50

publish:
  content_views:
    - RHEL Server
    - RHEL Workstation

promotion:
  lifecycle1:
    name: Quality
    content_views:
      - RHEL Server
      - RHEL Workstation

  lifecyclec2:
    name: Desktop QA
    content_views:
      - RHEL Workstation

cleanup:
  content_views:
    - view: RHEL Server
      keep: 1
    - view: RHEL Workstation
      keep: 3
